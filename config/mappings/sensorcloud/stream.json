{
	"stream" : {
		"_source" : {
			"enabled" : true
		},
		"_timestamp" : {
			"enabled" : true
		},
		"dynamic": "strict",
		"properties" : {
			"name" : {
				"type" : "multi_field",
				"fields" : {
					"name" : {
						"type" : "string",
						"index" : "analyzed"
					},
					"untouched" : {
						"type" : "string",
						"index" : "not_analyzed"
					}
				}
			},
			"description" : {
				"type" : "string",
				"index" : "no"
			},
			"type" : {
				"type" : "string",
				"index" : "not_analyzed"
			},
			"tags" : {
				"type" : "string",
				"index" : "analyzed"
			},
			"private" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"unit" : {
				"type" : "string",
				"index" : "not_analyzed"
			},
			"accuracy" : {
				"type" : "float",
				"index" : "not_analyzed"
			},
			"min_val" : {
				"type" : "float",
				"index" : "not_analyzed",
				"store" : "no"
			},
			"max_val" : {
				"type" : "float",
				"index" : "not_analyzed",
				"store" : "no"
			},
			"polling" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"uri" : {
				"type" : "string",
				"index" : "no"
			},
			"polling_freq" : {
				"type" : "long",
				"index" : "not_analyzed"
			},
			"location" : {
				"type" : "geo_point",
				"lat_lon" : true,
				"geohash" : true,
				"geohash_prefix" : true,
				"normalize" : false,
				"validate" : true
			},
			"resource" : {
				"properties" : {
					"resource_type" : {
						"type" : "string",
						"index" : "not_analyzed"
					},
					"uuid" : {
						"type" : "string",
						"index" : "no"
					}
				}
			},

			"quality" : {
				"type" : "float",
				"index" : "not_analyzed"
			},
			"active" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"user_ranking" : {
				"properties":{
					"average":{
						"type" : "float",
						"index" : "analyzed"
					},
					"nr_rankings":{
						"type": "long",
						"index": "no"
					}
				}
			},
			"subscribers" : {
				"type" : "long",
				"index" : "no",
				"store" : "not_analyzed"
			},
			"last_updated" : {
				"type" : "date",
				"index" : "not_analyzed"
			},
			"creation_date" : {
				"type" : "date",
				"index" : "not_analyzed"
			},
			"history_size" : {
				"type" : "long",
				"index" : "not_analyzed"
			},
			"user_id" : {
				"type" : "string",
				"index" : "not_analyzed"
			}
		}
	}
}
				
				
				
				
				
				
