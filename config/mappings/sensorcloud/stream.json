{
	"stream" : {
		"_source" : {
			"enabled" : true
		},
		"_timestamp" : {
			"enabled" : true
		},
		"properties" : {
			"resource_id" : {
				"type" : "string",
				"index" : "not_analyzed"
			},
			"name" : {
				"type" : "multi_field",
				"fields" : {
					"name" : {"type" : "string", "index" : "analyzed"},
					"untouched" : {"type" : "string", "index" : "not_analyzed"}
				}
			},
			"tags" : {
				"type" : "string",
				"index" : "analyzed"
			},
			"description" : {
				"type" : "string",
				"index" : "no"
			},
			"private" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"type" : {
				"type" : "string",
				"index" : "not_analyzed"
			},
			"accuracy" : {
				"type" : "float",
				"index" : "no"
			},
			"min_val" : {
				"type" : "float",
				"index" : "no",
				"store" : "no"
			},
			"max_val" : {
				"type" : "float",
				"index" : "no",
				"store" : "no"
			},
			"quality" : {
				"type" : "float",
				"index" : "not_analyzed"
			},
			"active" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"user_ranking" : {
				"type" : "double",
				"index" : "not_analyzed"
			},
			"subscribers" : {
				"type" : "long",
				"index" : "no",
				"store" : "no"
			},
			"last_updated" : {
				"type" : "date",
				"index" : "not_analyzed"
			},
			"creation_date" : {
				"type" : "date",
				"index" : "no"
			},
			"history_size" : {
				"type" : "long",
				"index" : "no"
			},
			"location" : {
				"type" : "geo_point",
				"lat_lon" : true,
				"geohash" : true,
				"geohash_prefix" : true,
				"normalize" : false,
				"validate" : true
			},
			"tags_suggest" : { "type" : "completion",
				"index_analyzer"  : "simple",      
				"search_analyzer" : "simple",
				"payloads" : false
			},
			"name_suggest" : { "type" : "completion",
				"index_analyzer"  : "simple",      
				"search_analyzer" : "simple",
				"payloads" : false
			},
			"type_suggest" : { "type" : "completion",
				"index_analyzer"  : "simple",      
				"search_analyzer" : "simple",
				"payloads" : false
			}

		}
	}
}
				
				
				
				
				
				
