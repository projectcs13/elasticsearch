{
	"stream" : {
		"_source" : {
			"enabled" : true
		},
		"_timestamp" : {
			"enabled" : true
		},
		"dynamic": "strict",
		"properties" : {
			"name" : {
				"type" : "multi_field",
				"fields" : {
					"name" : {"type" : "string", "index" : "analyzed"},
					"untouched" : {"type" : "string", "index" : "not_analyzed"}
				}
			},
			"tags" : {
				"type" : "string",
				"index" : "analyzed"
			},
			"description" : {
				"type" : "string",
				"index" : "no"
			},
			"private" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"unit" : {
				"type" : "string",
				"index" : "not_analyzed"
			},
			"accuracy" : {
				"type" : "float",
				"index" : "no"
			},
			"min_val" : {
				"type" : "float",
				"index" : "no",
				"store" : "no"
			},
			"max_val" : {
				"type" : "float",
				"index" : "no",
				"store" : "no"
			},
			"quality" : {
				"type" : "float",
				"index" : "not_analyzed"
			},
			"active" : {
				"type" : "boolean",
				"index" : "not_analyzed"
			},
			"user_ranking" : {
				"properties":{
					"average":{
						"type" : "float",
						"index" : "yes"
					},
					"nr_rankings":{
						"type": "long",
						"index": "no"
					}
				}
			},
			"subscribers" : {
				"type" : "long",
				"index" : "no",
				"store" : "no"
			},
			"last_updated" : {
				"type" : "date",
				"index" : "not_analyzed"
			},
			"creation_date" : {
				"type" : "date",
				"index" : "no"
			},
			"history_size" : {
				"type" : "long",
				"index" : "no"
			},
			"location" : {
				"type" : "geo_point",
				"lat_lon" : true
			},	
			"user_id" : {
				"type" : "string",
				"index" : "not_analyzed"
			},
			
			"uri" : {
				"type" : "string",
				"index" : "no"
			},
			"polling_freq" : {
				"type" : "long",
				"index" : "no"
			},
			"resource" : {
				"properties" : {
					"resource_type" : {
						"type" : "string",
						"index" : "not_analyzed"
					},
					"uuid" : {
						"type" : "string",
						"index" : "no"
					}
				}
			},
			"tags_suggest" : { "type" : "completion",
				"index_analyzer"  : "simple",      
				"search_analyzer" : "simple",
				"payloads" : false
			},
			"name_suggest" : { "type" : "completion",
				"index_analyzer"  : "simple",      
				"search_analyzer" : "simple",
				"payloads" : false
			},
			"type_suggest" : { "type" : "completion",
				"index_analyzer"  : "simple",      
				"search_analyzer" : "simple",
				"payloads" : false
			}

		}
	}
}
				
				
				
				
				
				
